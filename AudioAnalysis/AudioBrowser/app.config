<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline"/>
      </layout>
    </appender>
    <root>
      <level value="DEBUG"/>
      <appender-ref ref="ConsoleAppender"/>
    </root>
  </log4net>

  <appSettings>
    <!-- Audio utility locations-->
    <add key="AudioUtilityFfmpegExe" value="Executables\ffmpeg\ffmpeg.exe"/>
    <add key="AudioUtilityWvunpackExe" value="Executables\wavpack\wvunpack.exe"/>
    <add key="AudioUtilityMp3SpltExe" value="Executables\mp3splt\mp3splt.exe"/>
    <add key="AudioUtilitySoxExe" value="Executables\sox\sox.exe"/>

    <add key="DefaultSourceDir" value="Z:\Site27\"/>
    <add key="DefaultOutputDir" value="C:\SensorNetworks\WavFiles\SunshineCoast\Site27\"/>
    <!-- add key="DefaultAnalysisName" value="AcousticIndices"/-->
    <add key="DefaultAnalysisName" value="KiwiRecogniser"/>

    
    <!-- Program settings -->
    <!--
    <add key="DefaultSourceDir" value="I:\Projects\QUT\QutSensors\sensors-trunk\QutSensors.Test\TestData"/>
    <add key="DefaultOutputDir" value="I:\Projects\QUT\QutSensors\sensors-trunk\QutSensors.Test\TestData\output"/>
    
    <add key="DefaultSourceDir" value="C:\Sensors\Test audio"/>
    <add key="DefaultOutputDir" value="C:\Sensors\Test audio\output"/>
    -->

    <add key="AudacityExe" value="C:\Program Files (x86)\Audacity 1.3 Beta (Unicode)\audacity.exe"/>
    <add key="FrameLength" value="512"/>
    <add key="ResampleRate" value="17640"/>
    <add key="LowFreqBound" value="500"/>
    <add key="SegmentDuration" value="1"/>
    <add key="SegmentOverlap" value="0"/>
    <add key="FrameOverlap" value="0.0"/>
    <add key="SonogramBackgroundThreshold" value="4.0"/>
    <add key="TrackHeight" value="20"/>
    <add key="TrackCount" value="15"/>
    <add key="TrackNormalisedDisplay" value="true"/>
    <add key="SourceFileExt" value=".mp3"/>

  </appSettings>
  <startup>
    
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
</configuration>
