{
  "appSettings" : {
    // Audio utility locations for windows
    "AudioUtilityFfmpegExe": "audio-utils/windows/ffmpeg/ffmpeg.exe",
    "AudioUtilityFfprobeExe": "audio-utils/windows/ffmpeg/ffprobe.exe",
    "AudioUtilityWvunpackExe": "audio-utils/windows/wavpack/wvunpack.exe",
    "AudioUtilityMp3SpltExe": "audio-utils/windows/mp3splt/mp3splt.exe",
    "AudioUtilitySoxExe": "audio-utils/windows/sox/sox.exe",
    "AudioUtilityShntoolExe": "audio-utils/windows/shntool/shntool.exe",

    // Audio utility locations for linux
    "AudioUtilityFfmpegExeLinux": "/usr/local/bin/ffmpeg",
    "AudioUtilityFfprobeExeLinux": "/usr/local/bin/ffprobe",
    "AudioUtilityWvunpackExeLinux": "/usr/bin/wvunpack",
    "AudioUtilityMp3SpltExeLinux": "/usr/bin/mp3splt",
    "AudioUtilitySoxExeLinux": "/usr/bin/sox",
    "AudioUtilityShntoolExeLinux": "/usr/bin/shntool",

    // Audio utility locations for macos
    "AudioUtilityFfmpegExeMacOsX": "audio-utils/macosx/ffmpeg/ffmpeg",
    "AudioUtilityFfprobeExeMacOsX": "audio-utils/macosx/ffmpeg/ffprobe",
    "AudioUtilityWvunpackExeMacOsX": "audio-utils/macosx/wavpack/wvunpack",
    // mp3splt not supported on mac
    "AudioUtilityMp3SpltExeMacOsX": "",
    "AudioUtilitySoxExeMacOsX": "audio-utils/macosx/sox/sox",
    "AudioUtilityShntoolExeMacOsX": "",

    // Default target sample rate
    // chose this value because it is simple fraction (4/5) of 22050Hz
    // "DefaultTargetSampleRate": "17640" 
    "DefaultTargetSampleRate": "22050",
    "StandardFileDateFormat": "yyyyMMdd_HHmmsszzz",
    "StandardFileDateFormatUtc": "yyyyMMdd_HHmmssZ",
    "StandardFileDateFormatSm2": "yyyyMMdd_HHmmss"
  }
}