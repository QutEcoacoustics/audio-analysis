    <!-- Audio utility locations for windows -->
    <add key="AudioUtilityFfmpegExe" value="..\..\..\..\Extra Assemblies\ffmpeg\ffmpeg.exe" />
    <add key="AudioUtilityFfprobeExe" value="..\..\..\..\Extra Assemblies\ffmpeg\ffprobe.exe" />
    <add key="AudioUtilityWvunpackExe" value="..\..\..\..\Extra Assemblies\wavpack\wvunpack.exe" />
